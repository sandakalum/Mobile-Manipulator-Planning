<launch>

  <!--<include file="$(find tms_rc_double)/launch/includes/velocity_smoother.launch.xml"/>
  <include file="$(find tms_rc_double)/launch/includes/safety_controller.launch.xml"/>-->
  <arg name="odom_frame_id"   default="odom"/>
  <arg name="base_frame_id"   default="base_footprint"/>
  <arg name="global_frame_id" default="map"/>
  <arg name="odom_topic" default="odom" />
  <!--<arg name="laser_topic" default="scan" />-->
 <!--> <arg name="custom_param_file" default="$(find tms_rc_double)/param/dummy.yaml"/> -->

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

<!--
    <rosparam file="$(find tms_rc_double)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find tms_rc_double)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find tms_rc_double)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/base_local_planner_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/move_base_params.yaml" command="load" />
-->

    <rosparam file="$(find tms_rc_double)/param/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find tms_rc_double)/param/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find tms_rc_double)/param/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/dwa_local_planner_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/move_base_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/global_planner_params.yaml" command="load" />
    <rosparam file="$(find tms_rc_double)/param/navfn_global_planner_params.yaml" command="load" />



    <!-- external params file that could be loaded into the move_base namespace -->
    <!--<rosparam file="$(arg custom_param_file)" command="load" /> -->

    <!-- reset frame_id parameters using user input data -->
    <param name="global_costmap/global_frame" value="$(arg global_frame_id)"/>
    <param name="global_costmap/robot_base_frame" value="$(arg base_frame_id)"/>
  <!--  <param name="local_costmap/global_frame" value="$(arg odom_frame_id)"/> -->
    <param name="local_costmap/global_frame" value="$(arg global_frame_id)"/>
    <param name="local_costmap/robot_base_frame" value="$(arg base_frame_id)"/>
  <!--  <param name="DWAPlannerROS/global_frame_id" value="$(arg odom_frame_id)"/> -->
  --  <param name="DWAPlannerROS/global_frame_id" value="$(arg global_frame_id)"/>
  <!--  <remap from="cmd_vel" to="navigation_velocity_smoother/raw_cmd_vel"/> -->
  <!--  <remap from="cmd_vel" to="/cmd_vel_mux/input/keyop"/> -->
    <remap from="odom" to="$(arg odom_topic)"/>
    <!--<remap from="scan" to="$(arg laser_topic)"/>-->
  </node>
</launch>
